BU//0
10 PRINT LIN$(20)
20 GOSUB 8000
30 L1$="THE ICL 2903"
40 L2$="INTERACTIVE BASIC"
50 L3$="SYSTEM"
55 L4$="1: INTRODUCTION"
60 PRINT L1$&LIN$(1)&L2$&LIN$(1)&L3$&LIN$(1)&L4$
70 GOSUB 8000
99 STOP
8000 GOSUB 9000
8010 T=S9+10
8020 GOSUB 9000
8030 IF S9<T THEN 8020
8040 RETURN
9000 T9$=TIM$
9010 H9=VAL(SEG$(T9$,1,2))
9020 M9=VAL(SEG$(T9$,4,5))
9030 S9=VAL(SEG$(T9$,7,8))
9040 S9=H9*60*60+M9*60+S9
9050 RETURN
9999 END
