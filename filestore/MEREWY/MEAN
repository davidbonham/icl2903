BU
1000 C=0
1010 S1=0
1011 S2=0
1020 PRINT "WIDTH OF SQUARE";
1030 INPUT W
1040 PRINT "RESOLUTION";
1050 INPUT R
1060 FOR I=1 TO W STEP R
1070 FOR J=1 TO W STEP R
1080 FOR X=1 TO W STEP R
1090 FOR Y=1 TO W STEP R
1100 D1=X-I
1110 D2=Y-J
1120 D=SQR(D1*D1+D2*D2)
1130 S1=S1+D
1131 D1=W*RND-W*RND
1132 D2=W*RND-W*RND
1133 D=SQR(D1*D1+D2*D2)
1134 S2=S2+D
1140 C=C+1
1150 NEXT Y
1160 NEXT X
1170 PRINT C,S1/C,S2/C
1180 NEXT J
1190 NEXT I
1200 END
