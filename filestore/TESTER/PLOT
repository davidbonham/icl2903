BU//0
10 DIM P[50,50]
20 FOR X=0 TO 50
30 FOR Y=0 TO 50
40 D=(X-25)^2+(Y-25)^2
50 IF D<20^2 THEN 80
60 P[X,Y]=0
70 GOTO 90
80 P[X,Y]=1
90 NEXT Y
100 NEXT X
900 FOR Y=50 TO 0 STEP -1
910 FOR X=0 TO 50
920 IF P[X,Y]=1 PRINT "*";
930 IF P[X,Y]=0 PRINT " ";
940 NEXT X
950 PRINT 
960 NEXT Y
999 END
