BU//0
5 DIM D[100]
10 PRINT "WHAT CHARACTER IS TO BE USED";
20 INPUT C$
30 B$=MUL$(C$,50)
40 PRINT "HOW MANY ITEMS WILL BE ENTERED";
50 INPUT N
60 PRINT "ENTER THE";N;"ITEMS NOW"
70 FOR I=1 TO N
80 INPUT D[I]
85 IF D[I]<50 THEN 90
86 PRINT "TOO LARGE TO BE PLOTTED"
87 GOTO 80
90 NEXT I
100 PRINT "HERE IS THE HISTOGRAM :"
110 FOR I=1 TO N
120 PRINT SEG$(B$,1,D[I])
130 NEXT I
140 END
